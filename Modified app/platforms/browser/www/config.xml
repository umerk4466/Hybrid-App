<?xml version='1.0' encoding='utf-8'?>
<widget id="unigroup.mot_app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Mot_App</name>
    <description>
        Mot_App will remind you when your car insurnt or mot is finishing and also you can add multiple cars data in your personal account and can access at any time
    </description>
    <author email="umerk4466@gmail.com" href="https://motproject01.pythonanywhere.com/">
        motproject01.pythonanywhere.com Team
    </author>
    <content src="index.html" />
    <access origin=".*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="KeyboardShrinksView" value="true" />
</widget>
